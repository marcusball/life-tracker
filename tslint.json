{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "prefer-const": true,
        "arrow-return-shorthand": true,
        "jsdoc-format": true,
        "quotemark": [true, "single"],
        "no-console": false
    },
    "rules": {
        "prefer-const": true,
        "arrow-return-shorthand": true,
        "completed-docs": true,
        "interface-name": true,
        "jsdoc-format": true,
        "semicolon": [true, "always"],
        "quotemark": [true, "single"],
        "no-console": false,
        "max-classes-per-file": false,
        "no-unused-variable": true,
        "no-trailing-whitespace": [true, "ignore-comments"]
    },
    "rulesDirectory": []
}
<!doctype html5>
<html>

    <head>
        <title>COUNTERS</title>
    </head>

    <body>
        <h1>{{counter.name}}</h1>

        <div id="events">
            <table>
                {{#each events}}
                    <tr>
                        <td>{{quantity}} {{../counter.unit}}</td>
                        <td>{{timestamp}}</td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </body>

</html>